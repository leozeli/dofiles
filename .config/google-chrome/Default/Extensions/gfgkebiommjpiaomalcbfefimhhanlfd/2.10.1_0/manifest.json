{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "static/image/jjb.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "static/jquery.min.js", "static/content_script.js" ],
      "matches": [ "*://*.jd.com/*", "*://*.jd.hk/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "static/mobile_script.js" ],
      "matches": [ "*://*.m.jd.com/*", "*://m.jr.jd.com/*", "*://wq.jd.com/*", "*://wqs.jd.com/*", "*://msitepp-fm.jd.com/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "static/priceChart.js" ],
      "matches": [ "*://item.jd.com/*", "*://re.jd.com/*" ],
      "run_at": "document_end"
   } ],
   "description": "京价保是一个监控京东商品价格变化，即时提供价保申请信息的浏览器插件",
   "icons": {
      "128": "static/image/128.png",
      "16": "static/image/16.png",
      "48": "static/image/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "京价保 - 京东价保助手",
   "permissions": [ "*://*.jd.com/*", "*://*.jd.hk/*", "webRequest", "webRequestBlocking", "alarms", "contextMenus", "notifications" ],
   "short_name": "京价保",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.10.1",
   "web_accessible_resources": [ "static/touch-emulator.js" ]
}
