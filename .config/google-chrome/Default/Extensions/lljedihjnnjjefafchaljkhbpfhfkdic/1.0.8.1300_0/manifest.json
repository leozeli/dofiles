{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "assets/icons/icon128.png",
      "default_popup": "popup.html"
   },
   "commands": {
      "toggle-bionic": {
         "description": "Toggle bionic reading mode",
         "suggested_key": {
            "default": "Alt+B"
         }
      }
