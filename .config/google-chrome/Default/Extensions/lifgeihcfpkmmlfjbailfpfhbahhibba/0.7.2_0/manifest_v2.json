{
  "manifest_version": 2,
  "name": "Smart TOC",
  "version": "0.6.2",
  "description": "Webpage outliner",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Toggle table of contents"
  },
  "icons": {
    "16": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "toggle": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E",
        "chromeos": "Ctrl+Shift+E",
        "linux": "Ctrl+Shift+E"
      },
      "description": "Show/hide TOC"
    },
    "prev": {
      "description": "Jump to previous heading"
    },
    "next": {
      "description": "Jump to next heading"
    }
  },
  "options_ui": {
    "page": "options/index.html",
    "open_in_tab": false
  },
  "permissions": ["activeTab", "identity", "identity.email"],
  "author": "FallenMax@gmail.com"
}
