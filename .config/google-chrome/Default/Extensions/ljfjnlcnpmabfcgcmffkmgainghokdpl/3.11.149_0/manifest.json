{
   "action": {
      "default_icon": {
         "16": "icons/icon-16.png",
         "48": "icons/icon-48.png"
      },
      "default_title": "Burning Vocabulary"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "content_script.css" ],
      "js": [ "content_script.js" ],
      "matches": [ "http://*/*", "https://*/*", "ftp://*/*", "file://*/*" ],
      "run_at": "document_idle"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "__MSG_appDescription__",
   "homepage_url": "https://burningvocabulary.com",
   "host_permissions": [ "\u003Call_urls>", "*://*/*" ],
   "icons": {
      "128": "icons/icon-128.png",
      "16": "icons/icon-16.png",
      "48": "icons/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "options_ui": {
      "page": "dashboard.html"
   },
   "permissions": [ "tabs", "cookies", "storage", "contextMenus", "alarms", "notifications", "scripting" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.11.149",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "local-pdf-viewer.html", "dashboard.html", "icons/icon-128.png", "content_script.js", "content_script.css" ]
   } ]
}
